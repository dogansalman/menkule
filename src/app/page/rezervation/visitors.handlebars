{{#each visitor}}
    {{#if is_visitor}}
        <div data-index="{{@index}}" class="col-xs-12 visitor">
            <div data-id="" class="col-xs-11 disable_padding">
                <h3>{{fullname}} ({{gender}}) - {{tc}} </h3>
            </div>
            <div class="col-xs-1 disable_padding text-right remove-visitor"><a href="#">X</a> </div>
        </div>
    {{/if}}
{{/each}}